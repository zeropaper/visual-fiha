{
  "private": true,
  "name": "visual-fiha",
  "version": "0.0.1",
  "description": "Zeropaper's web based VJing tool",
  "main": "index.js",
  "engines": {
    "npm": "^3"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/zeropaper/visual-fiha.git"
  },
  "scripts": {
    "test": "karma start ./test/karma.conf.js --single-run",
    "clean": "rimraf *-build.js && rimraf *-build.js.map",
    "prebuild": "npm-run-all clean --parallel build:*",
    "build": "webpack --env.prod",
    "build:ace": "./.build-ace.sh",
    "postinstall": "npm run build",
    "prerun:dev": "npm run prebuild",
    "run:webpack": "webpack-dev-server --env.dev --port=8081",
    "run:karma": "karma start ./test/karma.conf.js",
    "run:wdio": "wdio ./test-e2e/wdio.conf.js",
    "run:dev": "npm-run-all --parallel run:webpack run:karma",
    "test:e2e": "npm-run-all build run:wdio",
    "_make-index": "node ./.markitdown.js",
    "_update-report": "plato -r -q -d report src",
    "prestage-master": "npm-run-all --parallel build _make-index _update-report",
    "stage-master": "git add index.html report/ ./worker-javascript.js ./test/expect.js ./*.worker.js ./*-build.js && git commit --allow-empty -m \"update builds\""
  },
  "author": "Valentin Vago <zeropaper@irata.ch>",
  "license": "MIT",
  "devDependencies": {
    "expect.js": "^0.3.1",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-mocha": "^1.3.0",
    "karma-webpack": "^2.0.1",
    "marked": "^0.3.6",
    "plato": "^1.7.0",
    "wdio": "latest",
    "wdio-mocha-framework": "^0.5.8",
    "wdio-selenium-standalone-service": "latest",
    "wdio-spec-reporter": "0.0.5",
    "wdio-static-server-service": "latest",
    "webpack-dev-server": "^2.2.0"
  },
  "dependencies": {
    "ace-builds": "^1.2.5",
    "ampersand-collection": "^2.0.0",
    "ampersand-events": "^2.0.2",
    "ampersand-router": "^4.0.0",
    "ampersand-state": "^5.0.2",
    "ampersand-view": "^10.0.1",
    "ampersand-view-switcher": "^2.1.0",
    "js-yaml": "^3.7.0",
    "localforage": "^1.4.3",
    "lodash.assign": "^4.2.0",
    "lodash.debounce": "^4.0.8",
    "lodash.throttle": "^4.0.0",
    "lodash.uniq": "^4.5.0",
    "npm-run-all": "^4.0.0",
    "ramda": "^0.23.0",
    "rimraf": "^2.5.4",
    "webpack": "^2.2.1",
    "worker-loader": "^0.7.1"
  }
}
